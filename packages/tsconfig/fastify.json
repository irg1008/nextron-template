{
  "display": "Fastify",
  "extends": "./app.json",
  "compilerOptions": {
    "target": "esnext",
    "module": "es2022",
    "pretty": true,
    "allowJs": true,
    "noEmitOnError": true,
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "noFallthroughCasesInSwitch": true,
    "useDefineForClassFields": true,
    "lib": ["esnext"],
    "sourceMap": true,
    "moduleResolution": "Node16"
  },
  "exclude": ["node_modules"]
}
